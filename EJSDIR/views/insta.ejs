<%- //include("includes/head.ejs") %> 
<%= //used include by making includes folder in views folder
    //Footer folder can also be made using include function(name,contact, etc)
%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Instagram</title>
    <link rel="stylesheet" href="/css/style.css">
    
</head>
<body>
    <h2>This page belongs to @<%= data.name %></h2>
    <button>follow</button>
    <button>message</button>

    <p>Followers : <%= data.followers %> &nbsp;&nbsp;&nbsp;&nbsp; Following : <%= data.following %></p>

    <hr/>

    <% for(let post of data.posts) { %>
    <img src="<%= post.image %>" alt="image">
    <p>Likes : <%= post.likes %> &nbsp;&nbsp;&nbsp;&nbsp; Comments : <%= post.comments %></p>
    <% } %>

    <%= //here comes the footer 
        //include("includes/footer.ejs")
        // To use create footer.ejs in /views/includes
    %><script src="/Js/app1.js"></script>
</body>
</html>
